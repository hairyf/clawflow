{
  "name": "nanobot-pm-engineering",
  "type": "module",
  "version": "0.0.0",
  "description": "Nanobot PM engineering CLI",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "nanobot-pm-engineering": "./bin/index.dev.mjs"
  },
  "files": ["bin", "dist"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "tsx src/cli/index.ts",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint ."
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "bin": {
      "nanobot-pm-engineering": "./bin/index.mjs"
    }
  },
  "dependencies": {
    "nanobot-pm": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "tsdown": "catalog:cli",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "vitest": "catalog:testing"
  }
}
